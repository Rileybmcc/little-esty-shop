<!DOCTYPE html>
<html>
  <head>
    <title>LittleEtsyShop</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%=yield%>
    <footer>
      <p> Repository name: little-esty-shop</p>
      <p>Contributors: </p>
      <ul>
        <% @commits.each do |user| %>
        <li> Username: <%= user[0]%> || Commits: <%= user[1] %> </li>
        <% end %>
      </ul>
    </footer>
  </body>
</html>
